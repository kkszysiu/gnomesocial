#: ../data/moblin-panel-myzone.desktop.in.in.h:1
msgid "Myzone panel"
msgstr "Panel Myzone"

#: ../data/myzone.desktop.in.in.h:1
msgid "Change the Myzone background and content"
msgstr "Ubah latar belakang dan isi Myzone"

#: ../data/myzone.desktop.in.in.h:2 ../capplet/myzone-cc-panel.c:157
msgid "Myzone"
msgstr "Myzone"

#: ../capplet/gconf-bridge.c:1232
#, c-format
msgid "GConf error: %s"
msgstr "Galat GConf: %s"

#: ../capplet/gconf-bridge.c:1237
msgid "All further errors shown only on terminal."
msgstr "Galat selanjutnya hanya ditampilkan pada terminal."

#: ../capplet/myzone-cc-panel.c:47
msgid "Show as much web service content as possible"
msgstr "Tampilkan isi layanan web sebanyak mungkin"

#: ../capplet/myzone-cc-panel.c:49
msgid "Show mostly web service content"
msgstr "Tampilkan lebih banyak isi layanan web"

#: ../capplet/myzone-cc-panel.c:51
msgid "Show half and half"
msgstr "Tampilkan separuh separuh"

#: ../capplet/myzone-cc-panel.c:53
msgid "Show mostly recent files"
msgstr "Tampilkan lebih banyak berkas terkini"

#: ../capplet/myzone-cc-panel.c:55
msgid "Show as many recent files as possible"
msgstr "Tampilkan berkas terkini sebanyak mungkin"

#: ../penge/penge-calendar-pane.c:473
msgid "Appointments"
msgstr "<b>Janji Temu</b>"

#: ../penge/penge-events-pane.c:68
msgid "Create an appointment"
msgstr "Buat suatu janji pertemuan"

#: ../penge/penge-events-pane.c:69 ../penge/penge-tasks-pane.c:52
msgid "Add"
msgstr "Tambah"

#: ../penge/penge-events-pane.c:361
msgid "No calendar entries this week"
msgstr "Tidak ada entri kalender minggu ini"

#: ../penge/penge-people-placeholder-tile.c:112
msgid "Things that your friends do online will appear here. Activate your accounts now!"
msgstr "Hal-hal yang dilakukan teman-teman Anda daring akan muncul di sini. Aktifkan akun Anda sekarang!"

#: ../penge/penge-recent-file-tile.c:215
msgid "Web page"
msgstr "Halaman web"

#: ../penge/penge-tasks-pane.c:51
msgid "Create a new task"
msgstr "Buat tugas baru"

#: ../penge/penge-tasks-pane.c:429
msgid "Nothing to do today"
msgstr "Tak ada yang perlu dilakukan hari ini"

#: ../penge/penge-task-tile.c:316
#, c-format
msgid "Due %x"
msgstr "Jatuh tempo %x"

#: ../penge/penge-welcome-tile.c:67
msgid "Welcome to MeeGo"
msgstr "Selamat datang ke MeeGo"

#: ../penge/penge-welcome-tile.c:86
msgid "This is the Myzone, where your recently used files and content from your web feeds will appear."
msgstr "Ini adalah Myzone, dimana berkas yang baru-baru ini dipakai dan isi dari asupan web akan muncul."

#: ../penge/penge-email-pane.c:193
msgid "Unread message"
msgstr "Pesan belum dibaca"

#: ../penge/penge-email-pane.c:195
msgid "Unread messages"
msgstr "Pesan-pesan belum dibaca"

#: ../penge/penge-email-pane.c:191
msgid "No unread messages"
msgstr "Tak ada pesan belum dibaca"

#: ../capplet/myzone-cc-panel.c:77
msgid "Do you want to clear your recent documents? This will remove the local content from the Myzone"
msgstr "Anda ingin membersihkan dokumen terkini? Ini akan menghapus muatan lokal dari Myzone"

#~ msgid "<b>Welcome to Moblin 2.1 for Netbooks</b>"
#~ msgstr "<b>Selamat Datang ke Moblin 2.1 untuk Netbook</b>"

